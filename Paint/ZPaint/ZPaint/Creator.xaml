        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZPaint"
        mc:Ignorable="d"
        Title="Creator" Height="300" Width="300">
    <Grid>
        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="235" VerticalAlignment="Top" Width="292" MouseLeftButtonUp="canvas_MouseLeftButtonUp" Background="White" MouseDown="canvas_MouseDown" MouseLeftButtonDown="canvas_MouseLeftButtonDown"/>

    </Grid>
</Window>
